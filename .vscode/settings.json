{
  "fileinator.ignoredFolders": [
    "target",
    ".git",
    "app/src/pages/temp",
    "app/src/constants/icons copy.rs",
    "app/src/constants/icons/icons_old.rs",
    "style/tailwind.config.js"
  ],
  "emmet.includeLanguages": {
    "rust": "html",
    "*.rs": "html"
  },
  "tailwindCSS.includeLanguages": {
    "rust": "html",
    "*.rs": "html"
  },
  "files.associations": {
    "*.rs": "rust"
  },
  "editor.quickSuggestions": {
    "other": "on",
    "comments": "on",
    "strings": true
  },
  "css.validate": false,
}
